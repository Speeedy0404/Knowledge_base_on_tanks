concept_gun_5_cm_KwK_39<-sc_node_not_relation;;
concept_gun_5_cm_KwK_39 => nrel_main_idtf: [пушка 5 cm KwK 39](*<- lang_ru;;*);;
concept_gun_5_cm_KwK_39 => nrel_main_idtf: [gun 5 cm KwK 39](*<- lang_en;;*);;


definition -> ...
(*
    -> rrel_key_sc_element: concept_gun_5_cm_KwK_39;;
    <= nrel_sc_text_translation:...
        (*
            -> [5 cm KwK 39  (5 cm Kampfwagenkanone 39 ) — германская танковая пушка калибра 50 мм, использовавшаяся главным образом в качестве 
            основного вооружения среднего танка PzKpfw III . Оснащённые ею машины принимали участие в боевых действиях во Второй мировой войне.](*<- lang_ru;;*);;

	        -> [The 5 cm KwK 39 (5 cm Kampfwagenkanone 39 ) was a German 50 mm tank gun used mainly as the main armament of the PzKpfw III medium tank. 
            The vehicles equipped with it took part in combat operations in the Second World War.](*<- lang_en;;*);;
        *);;
    => nrel_main_idtf: [Опр.(пушка 5 cm KwK 39)](*<- lang_ru;;*);;
    => nrel_main_idtf: [Def.(gun 5 cm KwK 39)](*<- lang_en;;*);;
*);;

